package third_party.staticperl

module stubs {
	source "stubs.c"
}

@Build(stage=2,script="$(EXTERNAL_MAKE)")
module build {
	depends embox.compat.libc.math_openlibm
	depends embox.compat.posix.LibPosix
	depends embox.compat.posix.time.time
	depends embox.compat.posix.util.times
	depends embox.compat.posix.util.nanosleep
	depends embox.driver.virtual.null
	depends stubs
}
