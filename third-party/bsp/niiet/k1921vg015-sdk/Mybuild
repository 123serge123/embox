package third_party.bsp.niiet

@BuildArtifactPath(cppflags="$(EXTERNAL_BUILD_DIR)/third_party/bsp/plib015/install")
@Build(stage=1,script="$(EXTERNAL_MAKE)")
static module plib015 {
	@AddPrefix("^BUILD/extbld/^MOD_PATH/install")
	@Cflags("-Wno-unused")
	@Cflags("-Wno-sequence-point")
	source
		"plib015_adcsar.c",
		"plib015_adcsd.c",
		"plib015_crc.c",
		"plib015_crypto.c",
		"plib015_dma.c",
		"plib015_gpio.c",
		"plib015_hash.c",
		"plib015_i2c.c",
		"plib015_qspi.c",
		"plib015_rcu.c",
		"plib015_spi.c",
		"plib015_trng.c",
		"plib015_uart.c",
		"plib015_wdt.c"
}
