package embox.mem

@DefaultImpl(NoPage)
abstract module page_api {
}

module NoPage extends page_api {
	source "no_page.h"
	option number page_size=4
}

module bitmask extends page_api {
	option string log_level="LOG_NONE"

	source "bitmask.c"
	source "bitmask.h"

	depends embox.util.Bitmap
	option number page_size=4096
}
