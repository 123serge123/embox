package embox.compat.libc

static module strerror {
	option number strerror_short = 0
	source "strerror.c"
}

@DefaultImpl(strerror_r_stub)
abstract module strerror_r {
}

static module strerror_r_stub extends strerror_r {
	source "strerror_r_stub.c"
}

static module str {
	source "memchr.c"
	source "memrchr.c"
	source "memcmp.c"
	source "memcpy.c"
	source "memccpy.c"
	source "memmove.c"
	source "memset.c"
	source "strcat.c"
	source "strchr.c"
	source "strchrnul.c"
	source "strcmp.c"
	source "strcoll.c"
	source "strxfrm.c"
	source "strcpy.c"
	source "strcspn.c"
	@NoRuntime depends strerror
	@NoRuntime depends strerror_r
	source "strlen.c"
	source "strlwr.c"
	source "strncat.c"
	source "strncmp.c"
	source "strncpy.c"
	source "strpbrk.c"
	source "strrchr.c"
	source "strspn.c"
	source "strstr.c"
	source "strcasestr.c"
	source "strupr.c"
	source "strcasecmp.c"
	source "strncasecmp.c"
	source "strtok.c"
	source "strlcpy.c"
	source "strnlen.c"
	source "ffs.c"
	source "bcopy.c"
	source "bzero.c"
	source "stpcpy.c"
}

static module str_dup {
	source "strdup.c"
	source "strndup.c"
	depends embox.mem.heap_api /*for strdup() */
}
