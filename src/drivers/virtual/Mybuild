package embox.driver.virtual

module null {
	source "null.c"

	depends embox.fs.driver.devfs
	depends embox.device.char_dev_api
}

module zero {
	source "zero.c"

	depends embox.compat.libc.all
	depends embox.fs.driver.devfs
	depends embox.device.char_dev_api
}

module mem {
	source "mem.c"

	depends embox.compat.libc.all
	depends embox.fs.driver.devfs
	depends embox.device.char_dev_api
	depends embox.kernel.task.idesc.idesc_mmap
}
